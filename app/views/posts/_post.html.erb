<div id="<%= dom_id post %>">
  <p>
    <strong>Id:</strong>
    <%= post.id %>
  </p>
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>
  <p>
    <strong>Content:</strong>
    <%= post.content %>
  </p>
  <p>
    <strong>Image:</strong>
    <%= image_tag post.image %>
  </p>
  <br>
  <p>
    <strong>Publicado por:</strong>
    <%= post.user.email %>
  </p>
</div>
